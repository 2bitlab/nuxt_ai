{
  "name": "nuxt_ai",
  "private": true,
  "type": "module",
  "scripts": {
    "prepare": "husky install",
    "upgrade": "nuxi upgrade --force",
    "-------lint---------": "----------------------------",
    "lint:all:eslint": "eslint --fix \"**/*.{ts,vue,js}\"",
    "lint:all:markdownlint": "pnpm lint:markdownlint \"docs/*.md\" \"*.md\"",
    "lint:all:prettier": "pnpm lint:prettier \"**/*.{js,json,css,scss,vue,html,md}\"",
    "lint:all:stylelint": "pnpm lint:stylelint \"**/*.{vue,scss}\"",
    "lint:eslint": "eslint --fix",
    "lint:markdownlint": "markdownlint",
    "lint:prettier": "prettier --write --log-level warn",
    "lint:stylelint": "stylelint --fix",
    "typecheck": "nuxt typecheck",
    "-------test---------": "----------------------------",
    "test": "vitest",
    "test:git": "vitest run --passWithNoTests",
    "coverage": "vitest run --coverage",
    "-----apps-------": "-----------------------------",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "-----db------": "-----------------------------------",
    "db:dev": "pnpm db:up && pnpm db:generate && pnpm db:migrate:deploy && pnpm db:update",
    "db:up": "docker compose -f docker-compose-dev.yml -p nuxt_ai_db_dev up -d",
    "db:nuke": "docker compose down --volumes --remove-orphans",
    "db:reset": "pnpm db:nuke && pnpm db:dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "tsx src/seed.ts",
    "db:format": "prisma format",
    "db:prisma:generate": "prisma generate",
    "db:ui": "prisma studio",
    "db:update": "pnpm run db:generate && pnpm run db:push",
    "db:generate": "zenstack generate --schema schema.zmodel",
    "zenstack:init": "npx zenstack init -p pnpm --prisma prisma/schema.prisma",
    "-----docker-------": "-----------------------------",
    "compose:up": "docker-compose up --build --force-recreate -d",
    "compose:build": "docker-compose build --force-rm --no-cache",
    "docker:build": "docker build -t nuxt_ai .",
    "docker:run": "docker run --env-file .env --name nuxt_ai -p 3000:3000 -d nuxt_ai",
    "docker:build:nginx": "docker build -f nginx/Dockerfile -t nuxt_ai_nginx .",
    "docker:run:nginx": "docker run --name nuxt_ai_nginx -p 8080:80 -d nuxt_ai_nginx"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@css-render/vue3-ssr": "^0.15.12",
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/tailwindcss": "^6.10.1",
    "@sidebase/nuxt-auth": "^0.6.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.10.0",
    "@types/nodemailer": "^6.4.14",
    "@types/prettier": "^3.0.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vueuse/nuxt": "^10.6.1",
    "@zenstackhq/openapi": "^1.3.2",
    "@zenstackhq/tanstack-query": "^1.3.2",
    "@zenstackhq/trpc": "^1.4.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-vue": "^9.18.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "markdownlint-cli": "^0.37.0",
    "naive-ui": "^2.35.0",
    "npm-run-all": "^4.1.5",
    "nuxt": "^3.8.2",
    "nuxt-icon": "^0.6.6",
    "postcss": "^8.4.31",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "^0.5.7",
    "prisma": "^5.6.0",
    "sass": "^1.69.5",
    "stylelint": "^15.11.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-tailwindcss": "^0.0.7",
    "stylelint-order": "^6.0.3",
    "tailwindcss": "^3.3.5",
    "vitest": "^0.34.6",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "zenstack": "^1.3.2"
  },
  "dependencies": {
    "@auth/core": "^0.18.4",
    "@auth/prisma-adapter": "1.0.9",
    "@hebilicious/authjs-nuxt": "^0.3.5",
    "@prisma/client": "^5.6.0",
    "@t3-oss/env-nuxt": "^0.7.1",
    "@tanstack/vue-query": "^5.8.7",
    "@trpc/client": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "@zenstackhq/runtime": "^1.3.2",
    "@zenstackhq/server": "^1.3.2",
    "@zenstackhq/swr": "^1.3.2",
    "ai": "^2.2.25",
    "bcryptjs": "^2.4.3",
    "cheerio": "1.0.0-rc.12",
    "cos-js-sdk-v5": "^1.4.21",
    "cos-nodejs-sdk-v5": "^2.12.5",
    "dayjs": "^1.11.10",
    "https-proxy-agent": "^7.0.2",
    "lodash-es": "^4.17.21",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.0",
    "qcloud-cos-sts": "^3.1.0",
    "redis": "^4.6.11",
    "superjson": "^2.2.1",
    "trpc-nuxt": "^0.10.13",
    "uuid": "^9.0.1",
    "x-crawl": "^8.3.0",
    "zod": "^3.22.4"
  }
}
